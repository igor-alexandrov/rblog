%h1
  Login

-form_for @user_session, :url => admin_user_session_path do |f|
  = f.error_messages
  = f.label :login
  %br
  = f.text_field :login
  %br
  
  = f.label :password
  %br
  = f.password_field :password
  %br
  = f.check_box :remember_me
  = f.label :remember_me
  %br
  = f.submit "Login"